<?php

use Symfony\Component\HttpFoundation\Response;

function code_highlighter_node_view(array &$build, \Drupal\Core\Entity\EntityInterface $entity, \Drupal\Core\Entity\Display\EntityViewDisplayInterface $display, $view_mode) {
    $build['body'][0]['#text'] = str_replace('node', 'replace_node', $build['body'][0]['#text']);
}